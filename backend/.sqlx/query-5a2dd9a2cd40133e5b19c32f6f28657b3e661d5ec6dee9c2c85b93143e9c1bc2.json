  "query": "\n            SELECT u.id, u.username, u.email, u.created_at, u.avatar_url,\n                   u.role as \"role: UserRole\", ac.password_hash as password\n            FROM users u\n            LEFT JOIN auth_credentials ac ON ac.user_id = u.id\n            WHERE u.id = $1\n            ",
