  "db_name": "PostgreSQL",
  "query": "\n                SELECT u.id, u.username, u.email, u.created_at,\n                       ac.password_hash as password, u.role as \"role: UserRole\"\n                FROM users u\n                LEFT JOIN auth_credentials ac ON u.id = ac.user_id\n                WHERE u.username = $1 AND ac.provider = 'basic'\n            ",
